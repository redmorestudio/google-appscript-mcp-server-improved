# Google OAuth2 Credentials
# Get these from https://console.cloud.google.com/apis/credentials
GOOGLE_APP_SCRIPT_API_CLIENT_ID=your_client_id_here
GOOGLE_APP_SCRIPT_API_CLIENT_SECRET=your_client_secret_here

# Logging level (error, info, debug)
LOG_LEVEL=error

# Environment
NODE_ENV=production

# OAuth callback port (must match Google Cloud Console redirect URI)
PORT=3001